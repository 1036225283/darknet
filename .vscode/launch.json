    "version": "0.2.0",
    "configurations": [
        {
            "name": "gcc-7 build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/darknet",
            "args": [
                "face_aliment",
                "test",
                "cfg/face_aliment.cfg",
                "~/work/data_set/helen/face_aliment_6000.weights",
                "~/work/data_set/helen/testset/3002247460_1.jpg"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "gcc-7 build active file",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}